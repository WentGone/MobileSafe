<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:padding="12dp">
	<ImageView 
	    android:id="@+id/item_listview_process_IV"
	    android:layout_width="50dp"
	    android:layout_height="50dp"/>
	<TextView 
	    android:id="@+id/item_listview_process_TV_Name"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_toRightOf="@id/item_listview_process_IV"
	    android:layout_marginLeft="10dp"
	    android:layout_centerVertical="true"
	    android:text="name"
	    android:textSize="16sp"
	    android:textColor="@color/black"/>
	<CheckBox 
	    android:id="@+id/item_listview_process_CB"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignParentRight="true"
	    android:layout_centerVertical="true"
	    android:focusable="false"
	    android:clickable="false"
	    android:focusableInTouchMode="false"
	    android:button="@drawable/process_radiobutton_button"/>
	<TextView 
	    android:id="@+id/item_listview_process_TV_Memory"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_toLeftOf="@id/item_listview_process_CB"
	    android:layout_centerVertical="true"
	    android:layout_margin="10dp"
	    android:text="memory"/>
</RelativeLayout>
